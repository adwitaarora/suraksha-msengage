<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/bootstrap.min.css">
    <style>
        .nav {
            cursor: pointer;
        }

        .nav-alt {
            cursor: pointer;
        }

        .hidden {
            display: none;
        }
    </style>
    <title>Error</title>
</head>

<body class="vh-100 d-flex flex-column justify-content-between">
    <%- include('partials/navbar') %>
        <div class="alert alert-danger w-50 offset-3" role="alert">
            <h4 class="alert-heading">
                Something went wrong, please try again
            </h4>
            <p> : (</p>
            <hr>
        </div>
        <%- include('partials/footer') %>
            <script>
                let navBrand = document.querySelector('.nav')
                let navBrandAlt = document.querySelector('.nav-alt')

                navBrand.addEventListener('click', () => {
                    navBrand.classList.toggle('hidden');
                    navBrandAlt.classList.toggle('hidden');
                })
                navBrandAlt.addEventListener('click', () => {
                    navBrand.classList.toggle('hidden');
                    navBrandAlt.classList.toggle('hidden');
                })
            </script>
</body>

</html>